<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.thoughtbot.expandablerecyclerview.sample">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">

    <activity android:name="com.thoughtbot.expandablerecyclerview.sample.MainActivity">

      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity
      android:name=".expand.ExpandActivity"
      android:parentActivityName=".MainActivity" />

    <activity
      android:name=".multicheck.MultiCheckActivity"
      android:parentActivityName=".MainActivity" />

    <activity
      android:name=".singlecheck.SingleCheckActivity"
      android:parentActivityName=".MainActivity" />

    <activity
      android:name=".multitype.MultiTypeActivity"
      android:parentActivityName=".MainActivity" />

    <activity
      android:name=".multitypeandcheck.MultiTypeCheckGenreActivity"
      android:parentActivityName=".MainActivity" />
    <activity android:name=".custom.CustomExpandActivity" />

  </application>

</manifest>
